{
  "name": "variable-app",
  "version": "0.5.1",
  "private": true,
  "scripts": {
    "analyze": "vite-bundle-visualizer",
    "dev": "vite",
    "start": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prettify": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json}\"",
    "eslint": "eslint src --ext \".js,.jsx,.ts,.tsx\"",
    "eslint-fix": "eslint src --ext \".js,.jsx,.ts,.tsx\" --fix",
    "stylelint": "npx stylelint \"src/**/*.scss\" --color",
    "stylelint-fix": "npx stylelint \"src/**/*.scss\" --color --fix",
    "lint:staged": "lint-staged",
    "lint:ts": "npm run eslint",
    "lint:format": "prettier --check './{src,cypress}/**/*.{ts,tsx}'",
    "lint:format:fix": "prettier --write './{src,cypress}/**/*.{ts,tsx}'",
    "lint:ts:fix": "npm run lint:ts -- --fix",
    "lint:css": "npm run stylelint",
    "lint": "tsc && npm run lint:ts && npm run lint:css",
    "prepare": "husky install",
    "postinstall": "patch-package"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.scss": [
      "npm run stylelint"
    ],
    "src/**/*.{js,jsx,ts,tsx}": [
      "npm run prettify",
      "npm run eslint"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@d8x/perpetuals-sdk": "1.0.2",
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@ethersproject/providers": "5.7.2",
    "@lifi/widget": "2.9.2",
    "@mui/icons-material": "5.14.3",
    "@mui/material": "5.14.4",
    "@mui/utils": "5.14.8",
    "@noble/secp256k1": "2.0.0",
    "@rainbow-me/rainbowkit": "1.0.11",
    "@wagmi/core": "1.4.2",
    "buffer": "6.0.3",
    "classnames": "2.3.2",
    "core-js": "3.32.0",
    "crypto-js": "4.2.0",
    "cryptocurrency-icons": "0.18.1",
    "date-fns": "2.30.0",
    "geonames.js": "3.0.6",
    "html-to-image": "1.11.11",
    "i18next": "23.4.1",
    "i18next-browser-languagedetector": "7.1.0",
    "i18next-http-backend": "2.2.1",
    "jotai": "2.3.1",
    "lightweight-charts": "4.0.1",
    "lightweight-charts-react-wrapper": "1.3.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet-async": "1.3.0",
    "react-i18next": "13.0.3",
    "react-minimal-pie-chart": "8.4.0",
    "react-resize-detector": "8.1.0",
    "react-router-dom": "6.14.2",
    "react-secure-storage": "1.3.0",
    "react-toastify": "9.1.3",
    "react-world-flags": "1.5.1",
    "viem": "1.11.1",
    "wagmi": "1.4.2"
  },
  "devDependencies": {
    "@types/crypto-js": "4.1.2",
    "@types/react": "18.2.25",
    "@types/react-dom": "18.2.7",
    "@types/react-world-flags": "1.4.2",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "@vitejs/plugin-react": "4.1.0",
    "eslint": "8.46.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-react": "7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "8.0.3",
    "lint-staged": "13.2.3",
    "patch-package": "8.0.0",
    "postinstall-postinstall": "2.1.0",
    "prettier": "3.0.1",
    "sass": "1.65.1",
    "stylelint": "14.16.1",
    "stylelint-config-css-modules": "4.3.0",
    "stylelint-config-standard-scss": "6.1.0",
    "stylelint-order": "6.0.3",
    "typescript": "5.1.6",
    "vite": "4.4.8",
    "vite-bundle-visualizer": "0.10.0",
    "vite-plugin-checker": "0.6.2",
    "vite-plugin-svgr": "3.2.0",
    "vite-tsconfig-paths": "4.2.0"
  }
}
